{
  "name": "storybook-mirage",
  "version": "0.0.4",
  "description": "A Storybook addon to use and interact with a MirageJS server",
  "keywords": [
    "storybook-addons",
    "data-state",
    "test",
    "API",
    "mocking",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HipsterBrown/storybook-mirage"
  },
  "author": "HipsterBrown <headhipster@hipsterbrown.com>",
  "license": "MIT",
  "main": "dist/preset",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "babel ./src --out-dir ./dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "start": "concurrently \"npm run storybook -- --no-manager-cache --quiet\" \"npm run build -- --watch\"",
    "build-storybook": "build-storybook",
    "prerelease": "node check-metadata.js",
    "release": "npm run build && auto shipit"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@storybook/react": "^6.2.7",
    "auto": "^10.3.0",
    "babel-loader": "^8.1.0",
    "chalk": "^2.4.2",
    "concurrently": "^5.3.0",
    "miragejs": "^0.1.41",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "@storybook/addons": "^6.2.7",
    "@storybook/api": "^6.2.7",
    "@storybook/components": "^6.2.7",
    "@storybook/core-events": "^6.2.7",
    "@storybook/theming": "^6.2.7",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0",
    "miragejs": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "MirageJS",
    "unsupportedFrameworks": [
      "react-native"
    ],
    "icon": "https://avatars.githubusercontent.com/u/47899903?s=200&v=4"
  }
}
